<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#7E57c2"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_sidweb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginLeft="160dp"
        android:onClick="irSidweb"
        android:text="Sidweb"
        app:layout_constraintBottom_toTopOf="@+id/btn_academico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.7" />

    <Button
        android:id="@+id/btn_academico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginLeft="152dp"
        android:layout_marginBottom="36dp"
        android:onClick="irAcademico"
        android:text="Academico"
        app:layout_constraintBottom_toTopOf="@+id/btn_vinculo"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_vinculo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginLeft="160dp"
        android:layout_marginBottom="44dp"
        android:onClick="irVinculo"
        android:text="Vinculo"
        app:layout_constraintBottom_toTopOf="@+id/btn_bienestar"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_bienestar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginLeft="156dp"
        android:onClick="irBienestar"
        android:text="Bienestar"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="433dp" />

</androidx.constraintlayout.widget.ConstraintLayout>